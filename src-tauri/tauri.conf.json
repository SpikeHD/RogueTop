{
  "productName": "RogueTop",
  "version": "0.1.0",
  "identifier": "com.spikehd.roguetop",
  "build": {
    "beforeDevCommand": "pnpm build",
    "devUrl": "https://pokerogue.net/",
    "beforeBuildCommand": "pnpm build",
    "frontendDist": "https://pokerogue.net/"
  },
  "app": {
    "windows": [
      {
        "title": "PokeRogue",
        "width": 1280,
        "height": 720
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
